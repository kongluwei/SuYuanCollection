<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="collection.suyuan.pdwy.com.suyuancollection">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- 登陆页面 -->
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <!-- 主页面 -->
        <activity android:name=".MainActivity"></activity>
        <!-- 任务基本信息页面 -->
        <activity android:name=".activity.TaskBasicInformationActivity"></activity>
        <!-- 密码修改页面 -->
        <activity android:name=".activity.PasswordModificationActivity"></activity>
        <!-- 版本信息页面 -->
        <activity android:name=".activity.VersionInformationActivity"></activity>
        <!-- 功能介绍页面 -->
        <activity android:name=".activity.FunctionalIntroductionActivity"></activity>


    </application>

</manifest>